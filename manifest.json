{
  "manifest_version": 2,
  "name": "Search helper",
  "version": "1",
  "description": "Modifies search query to get more relevant results",
  "author": "Eldar Shikhbadinov",
  "browser_action": {
    "default_title": "Search helper",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*.google.com/",
    "https://*.google.com/"
  ],
  "minimum_chrome_version": "22",
  "offline_enabled": true,
  "omnibox": {
    "keyword": "help"
  }
}